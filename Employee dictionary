Std={ }
n=int(input('enter number of employees :))
for i in range(1,n+1):
  l=[ ]
  name=input('enter  employee name')
  l.append(name)
  desig=input('enter employee desig')
  l.append(desig)
  salary=int(input('enter employee salary')) 
  l.append(salary)
  phno=int(input('enter employee phno')) 
  l.append(phno)
  Std[i]=l
  Print(Std)
